{
  "extends": ["next/core-web-vitals", "next", "prettier"],
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "quotes": ["error"],
    "semi": ["error"]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
